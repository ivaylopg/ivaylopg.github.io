{
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "Modernizr": true,
      "p5": true,
      "Vector2d": true,
      "rot13rot5Encode": true,
      "Cube":true
    },
    "rules": {
      "linebreak-style": [
        2,
        "unix"
      ],
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-console": 1
    }
  }
}
