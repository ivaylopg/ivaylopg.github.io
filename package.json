{
  "name": "IvayloGetovDotCom",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "jquery": true
    },
    "globals": {
      "Modernizr": true,
      "p5": true,
      "Vector2d": true,
      "rot13rot5Encode": true,
      "Cube": true
    },
    "rules": {
      "linebreak-style": [
        2,
        "unix"
      ],
      "no-unused-vars": 1,
      "no-empty": 1,
      "no-console": 1
    }
  },
  "scripts": {
    "serve": "bundle exec jekyll serve",
    "serveinc": "bundle exec jekyll serve --incremental"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^3.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-uglify": "^3.0.2"
  }
}
